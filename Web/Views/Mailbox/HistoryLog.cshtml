@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="warper container-fluid historylog">
    <div class="page-header">
        <h1>History Log</h1>
    </div>
    <div class="row">
        <div class="col-sm-12">
      
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="table-responsive">
                        <table id="sent-files-datatable" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th><i class="fa fa-ban"></i> Access Cancelled</th>
                                    <th>Log Date/Time</th>
                                    <th>File Name</th>
                                    <th>File ID</th>
                                    <th>Size</th>
                                    <th>Uploaded to</th>
                                    <th>Company</th>
                                    <th>Comments</th>
                                    <th>Expire Date</th>
                                    <th>Download</th>
                                </tr>
                            </thead>
                            <tbody colspan="1" class="dataTables_empty"></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<iframe style="display: none;" id="fileDelegate">
    <form method="post" action="/api/files">
        <input type="hidden" name="criteria" />
    </form>
</iframe>

<script src="~/src/js/app/component/mailbox/HistoryLogVM.js"></script>